%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%  Test CISD
%  Electronic Hartree Fock
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

SYSTEM_DESCRIPTION='Molecula de Li'

GEOMETRY
e-(Li)  AUG-CC-PVQZ	0.000	0.000	0.000 addParticles=-1
Li    	dirac	0.000	0.000	0.0000 
END GEOMETRY

TASKS
	method = "UHF"
	configurationInteractionLevel ="FCI"
END TASKS

CONTROL
readCoefficients=F
!numberOfCores=1
	scfelectronicenergytolerance=1E-8
        CIdiagonalizationMethod="DSYEVX"
        !CIdiagonalizationMethod="ARPACK"
        !CIdiagonalizationMethod="JADAMILU"
	!CISaveEigenvector = true
	!CILoadEigenvector = true
END CONTROL


INPUT_CI
	species="E-ALPHA" core=0 active=0 
	species="E-BETA" core=0 active=0 
END INPUT_CI

